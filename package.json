    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
